every second:
  loop all players:
    world of loop-player is "ul_admin"
    loop-player's y-coordinate < -32
    teleport loop-player to location(loop-player's x-coordinate, 500, loop-player's z-coordinate, world "world_the_end")

on damage:
  world of victim is "ul_admin"
  victim is player
  damage cause is void
  cancel event